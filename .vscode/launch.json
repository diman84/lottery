    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",        
            "preLaunchTask": "compile",    
            "name": "Fetch Hmtl",
            "program": "${workspaceFolder}/src/fetcher.js",
            "outFiles": [
                "${workspaceRoot}/.compiled/**/*.js"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",        
            "preLaunchTask": "compile",    
            "name": "Parse Hmtl",
            "program": "${workspaceFolder}/src/parser.js",
            "outFiles": [
                "${workspaceRoot}/.compiled/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]    
}